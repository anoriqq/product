#!/bin/bash

FUNCTION_FILE=${WORKSPACE_ROOT}/scripts/functions.sh
source ${FUNCTION_FILE}



log "[Run]: $1 ${*:2}"

if grep -e "^function _${1}.*{$" $FUNCTION_FILE &> /dev/null
then
  COMMAND=${1:-help}
else
  COMMAND=help
  log "[Error]: ${1}: command no found"
fi

_${COMMAND} ${*:2} || (log "[Error]: ${COMMAND} command failed"; exit 1)
