{
  "watch": [
    "src"
  ],
  "ignore": [
    ".git",
    "node_modules",
    "dist",
    ".vscode",
    "view"
  ],
  "env": {
    "NODE_ENV": "development",
    "DEBUG": "express:application,node,request,app:*",
    "PORT": "8000"
  },
  "exec": "ts-node --project $PRODUCT_DIR/server/tsconfig.json $PRODUCT_DIR/server/src/index.ts",
  "ext": "js,json,ts",
  "verbose": true
}
